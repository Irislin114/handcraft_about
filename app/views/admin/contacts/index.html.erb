<h1>Contacts list<h1>

<ul>
<% @contacts.each do |contact| %>
  <li>
    <%= link_to contact.subject, admin_contact_path(contact) %>
    <%= link_to '編輯', edit_admin_contact_path(contact) %>
    <%= link_to '刪除', admin_contact_path(contact), method: 'delete'%>
    <!-- <%= link_to 'hello <em>hey</em>'.html_safe, '/about' %> -->
  </li>
<% end %>
</ul>
